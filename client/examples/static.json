{
  "name": "Static example",
  "versions": {
    "v1": {
      "routes": [
        {
          "accept": {
            "method": "GET",
            "url": "/"
          },
          "proxy": {
            "via": "static",
            "body": "Hello {{query.name}}"
          }
        }
      ]
    }
  }
}
